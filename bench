#!/bin/sh
echo "Very cool language benchmark script"
echo "---"
echo "Node.js"
node perf.js
echo "---"
echo "Lua (LuaJIT)"
luajit perf.lua
echo "---"
echo "Rust"
cd perfrustsrc
cargo run -q --release
cd ..
echo "---"
echo "Go"
go run perf.go
echo "---"
echo "Python (PyPy)"
pypy perf.py
echo "---"
echo "C (GCC)"
gcc perf.c -Ofast -march=native -mtune=native -o perfc
./perfc
echo "---"
echo "C++ (G++)"
g++ perf.cpp -march=native -mtune=native -Ofast -o perfcpp
./perfcpp
echo "---"
echo "C#"
cd perfcsharpsrc
dotnet run --configuration Release --verbosity quiet
cd ..
echo "---"
echo "Java"
javac perf.java
java perf
echo "---"
